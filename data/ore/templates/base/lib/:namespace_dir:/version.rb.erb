<%- @modules.each_with_index do |namespace,index| -%>
<%= '  ' * index %>module <%= namespace %>
<%- end -%>
<%= '  ' * @module_depth %># <%= @name %> version
<%= '  ' * @module_depth %>VERSION = <%= @version.dump %>
<%- (@module_depth - 1).downto(0) do |index| -%>
<%= ' ' * index %>end
<%- end -%>
